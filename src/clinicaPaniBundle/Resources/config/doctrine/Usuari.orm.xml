      <unique-constraint name="UNIQ_68CC94FF68CC94FF" columns="usuari"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="usuari" type="string" column="usuari" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pass" type="string" column="pass" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rol" type="integer" column="rol" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
